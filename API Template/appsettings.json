{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Token": {
    "SecretKey": "YourSecretKey",
    "RefreshKey": "YourRefreshKey",
    "AudienceKey": "YourAudienceKey",
    "IssuerKey": "YourIssuerKey"
  },
  "Database": {
    "ConnectionString": "Server=; Database=; Trusted_Connection=True; MultipleActiveResultSets=true"
  },
  "ResponseTime": {
    "Enabled": true,
    "MilisecondsElapsedToNotify": 5000
  }
}
